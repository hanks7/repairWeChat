<!--pages/myInfor/myInfor.wxml-->
<view class="main">
  <image class="headimg" mode="aspectFit" src="/images/logo.png"></image>
  <text class="style-text name" >登录人名称：{{userInfo.LoginName}}</text>
  <text class="style-text journey">登录人token：{{userInfo.Token}}</text>
</view>

<view class="item" bindtap="clickIntentMyorder">我的订单</view>
<view class="item" bindtap="clickIntentMyorder">公司派单</view>
<view class="item" bindtap="clickIntentMyorder">需要确认的订单</view>
<view class="item" bindtap="clickQuit">退出登录</view>
 